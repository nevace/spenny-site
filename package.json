{
	"name": "spenny-site",
	"private": true,
	"description": "Spenny website",
	"version": "0.1.0",
	"author": "Liam Neville",
	"dependencies": {
		"babel-plugin-styled-components": "^1.10.0",
		"gatsby": "^2.1.19",
		"gatsby-image": "^2.0.30",
		"gatsby-plugin-google-analytics": "^2.0.16",
		"gatsby-plugin-manifest": "^2.0.20",
		"gatsby-plugin-offline": "^2.0.24",
		"gatsby-plugin-prefetch-google-fonts": "^1.4.0",
		"gatsby-plugin-react-helmet": "^3.0.7",
		"gatsby-plugin-sharp": "^2.0.23",
		"gatsby-plugin-styled-components": "^3.0.6",
		"gatsby-source-filesystem": "^2.0.23",
		"gatsby-transformer-sharp": "^2.1.15",
		"prop-types": "^15.7.2",
		"react": "^16.8.3",
		"react-dom": "^16.8.3",
		"react-helmet": "^5.2.0",
		"react-ionicons": "^3.0.1",
		"react-mailchimp-subscribe": "^2.1.0",
		"react-select": "^2.4.1",
		"styled-components": "^4.1.3",
		"styled-media-query": "^2.1.1",
		"whatwg-fetch": "^3.0.0"
	},
	"devDependencies": {
		"babel-preset-gatsby": "^0.1.8",
		"eslint-config-prettier": "^4.1.0",
		"eslint-plugin-prettier": "^3.0.1",
		"eslint-plugin-react": "^7.12.4",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.5",
		"prettier": "^1.16.4",
		"stylelint": "^9.10.1",
		"stylelint-config-prettier": "^5.0.0",
		"stylelint-prettier": "^1.0.6"
	},
	"scripts": {
		"build": "gatsby build",
		"develop": "gatsby develop",
		"format": "prettier --write src/**/*.{js,jsx}",
		"start": "npm run develop",
		"serve": "gatsby serve"
	},
	"husky": {
		"hooks": {
			"postcommit": "git update-index --again",
			"precommit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix",
			"git add"
		],
		"*.css": [
			"stylelint --fix",
			"git add"
		]
	}
}
